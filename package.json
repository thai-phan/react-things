{
  "name": "react-created",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.4",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.12",
    "redux": "^3.7.2",
    "voucher-code-generator": "^1.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/output",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/output --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "font-awesome": "^4.7.0",
    "node-sass": "^4.5.3",
    "react-redux": "^5.0.6",
    "sass-loader": "^6.0.6",
    "webpack": "^3.5.5"
  }
}
